{
  "name": "@okta/samples-js-angular",
  "version": "0.0.4",
  "description": "Angular Sample Applications for Okta",
  "scripts": {
    "lint:okta-hosted-login": "yarn --cwd okta-hosted-login lint",
    "lint:custom-login": "yarn --cwd custom-login lint",
    "lint": "yarn lint:okta-hosted-login && yarn lint:custom-login",
    "test": "yarn test:e2e",
    "test:e2e": "yarn test:okta-hosted-login && yarn test:custom-login",
    "okta-hosted-login-server": "yarn --cwd okta-hosted-login start",
    "test:okta-hosted-login": "export TEST_TYPE=implicit || setx TEST_TYPE \"implicit\" && protractor protractor.conf.js --sample=okta-hosted-login --disableChecks",
    "custom-login-server": "yarn --cwd custom-login start",
    "test:custom-login": "export TEST_TYPE=implicit || setx TEST_TYPE \"implicit\" && protractor protractor.conf.js --sample=custom-login --disableChecks",
    "resource-server": "node scripts/startResourceServer.js",
    "preresource-server": "yarn setup-env",
    "pretest": "node ./scripts/update-se-drivers.js && yarn setup-env",
    "pretest-legacy": "yarn webdriver-update-legacy && yarn setup-env",
    "postinstall": "yarn install-custom-login && yarn install-okta-hosted-login",
    "setup-env": "node scripts/setup-env.js",
    "install-custom-login": "cd custom-login && yarn install",
    "install-okta-hosted-login": "cd okta-hosted-login && yarn install",
    "webdriver-update-legacy": "webdriver-manager update --versions.standalone=3.141.59 --versions.chrome 2.45 --gecko false",
    "build:okta-hosted-login": "cd ./okta-hosted-login && yarn install --production=false && yarn build",
    "build:custom-login": "cd ./custom-login && yarn install --production=false && yarn build",
    "build": "yarn build:okta-hosted-login && yarn build:custom-login",
    "start": "node ./heroku/server.js",
    "dev": "nodemon ./heroku/server.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/samples-js-angular.git"
  },
  "bugs": "https://github.com/okta/samples-js-angular/issues",
  "homepage": "https://github.com/okta/samples-js-angular",
  "devDependencies": {
    "axios": "^0.27.2",
    "dotenv": "^8.2.0",
    "find-process": "^1.4.3",
    "forever-monitor": "^3.0.3",
    "jasmine-reporters": "^2.5.0",
    "minimist": "^1.2.6",
    "nodemon": "^2.0.16",
    "platform": "^1.3.6",
    "protractor": "^7.0.0",
    "wait-on": "^6.0.1"
  },
  "bin": {},
  "dependencies": {
    "express": "^4.17.1"
  },
  "private": true
}
